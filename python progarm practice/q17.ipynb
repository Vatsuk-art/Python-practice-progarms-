   "source": [
    "#Generate the Fibonacci series upto nth term\n",
    "n= int(input(\"enter a number \"))\n",
    "a=0\n",
    "b=1\n",
    "c=0\n",
    "while True:\n",
    "    c=a+b\n",
    "    a=b\n",
    "    b=c\n",
    "    \n",
    "    if a>=n:\n",
    "        break\n",
    "    else:\n",
    "        print(a,\",\",end=\"\")\n",
    "\n",
    "    "
